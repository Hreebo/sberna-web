              #if(material.oldPrice == nil) {
                není záznam
              } else {
                #if(material.oldPrice > 0) {
                  <font color="green"> &#8593; #(material.oldPrice) % </font>
                }
                #if(material.oldPrice < 0) {
                  <font color="red"> &#8595; #(material.oldPrice) % </font>
                }
